<resources>

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="CustomTheme" parent="Theme.Sherlock.Light.DarkActionBar"></style>

    <style name="UserNameTextStyle">
        <item name="android:textSize">@dimen/text_18_size</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="LineStyle">
        <item name="android:background">#33ffffff</item>
        <item name="android:layout_height">@dimen/main_bottom_line</item>
    </style>

    <style name="LineStyle2">
        <item name="android:background">#33A6A6A6</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="LineStyle3">
        <item name="android:background">#354F4F4F</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="SlidingItemTextStyle">
        <item name="android:textSize">@dimen/text_20_size</item>
        <item name="android:textColor">@color/sliding_item_color</item>
    </style>

    <style name="GuidanceLineStyle">
        <item name="android:background">@color/guidance_text_color</item>
        <item name="android:layout_height">@dimen/main_bottom_line</item>
    </style>
    <style name="SettingUserNameTextStyle">
        <item name="android:textSize">@dimen/text_17_size</item>
        <item name="android:textColor">@color/black</item>
    </style>
    
    <style name="SettingItemNameTextStyle">
        <item name="android:textSize">@dimen/text_15_size</item>
        <item name="android:textColor">@color/black</item>
    </style>

</resources>